#include <iostream>
using namespace std;

int factorial(int n) {

    if (n <= 1) {
        return 1;
    }

    int result = 1;

    for (int i = 2; i <= n; i++) {
        result *= i;
    }

    return result;
}

int main() {
    int number;

    std::cout << "Enter a non-negative integer: ";
    std::cin >> number;

    if (number < 0) {
        std::cout << "Error: impossible for negative numbers." << std::endl;
        return 1;
    }

    int result = factorial(number);
    std::cout << "Factorial of " << number << " is " << result << std::endl;

    return 0;
}
